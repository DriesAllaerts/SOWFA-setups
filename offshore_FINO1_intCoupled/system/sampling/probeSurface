probeSurface
{
    type            probes;
    functionObjectLibs ( "libsampling.so" );
    name            probeSurface;
    interpolationScheme cellPointFace;
    outputControl   timeStep;
    outputInterval  1;
    enabled         true;
    fields          ( Rwall qwall );
    probeLocations  ( #include "coordinateFiles/fino_coordinates_surfaceInfo_Pert.txt" );
}


